package com.example.draganddrop;

import org.cocos2d.types.CGPoint;

public class Number {
     MySprite num1;
     MySprite num2;
	public Number(int number,int size,CGPoint position)
	{
		
	   num1=new MySprite("stageNumber.png",true,position);
	   num2=new MySprite("stageNumber.png",true,position+CGPoint.make(size, 0));
	}
}
