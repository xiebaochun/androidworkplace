package com.example.draganddrop;

import org.cocos2d.nodes.CCSprite;

public class MySprite{
	CCSprite sprite;
	public MySprite(String filePath,int positionX,int positionY)
	{
		sprite=CCSprite.sprite(filePath+".png");
		sprite.setAnchorPoint(0,0);
		sprite.setPosition(QUESTION_POSITION_X*SIZE_RATE_X,QUESTION_POSITION_Y*SIZE_RATE_Y);
		float d=CommonItem.SIZE_RATE_X;
	}
	
}
